    // "features": {},
    // ðŸ‘‡ Use 'forwardPorts' to make a list of ports inside the container available locally.
    "forwardPorts": [
        6006 // Arize Phoenix Web UI
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "njpwerner.autodocstring"
            ]
        }
    },
    "updateContentCommand": "./.devcontainer/update_content_command.sh"
